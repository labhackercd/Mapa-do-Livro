<div class="main">
  <div class="row">
    <div class="col s6">
      <div class="logo">
        <%= image_tag("logo.png", :alt => "logo") %>
      </div>
      <div class="map">
        <div class="serie">
          <span>Mostrando: </span><span class="sn"><%= @dashboard.description %></span>
          
          <!-- Dropdown Trigger -->
          <a class='dropdown-button btn cyan' href='' data-activates='serie_dropdown'>Série</a>

          <!-- Dropdown Structure -->
          <ul id='serie_dropdown' class='dropdown-content'>
            <li><a class="seriea" href="/dashboards/recursos-ideb-4s">4ª Série do Ensino Fundamental</a></li>
